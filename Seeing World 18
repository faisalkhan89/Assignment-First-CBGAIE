let placesToVisit: string[] = ["Japan", "Brazil", "France", "Australia", "Canada"];

// Print original array
console.log("Original array:");
console.log(placesToVisit);

// Print array in alphabetical order without modifying the original list
console.log("\nArray in alphabetical order:");
console.log([...placesToVisit].sort());

// Print original array to show it's unchanged
console.log("\nOriginal array (unchanged):");
console.log(placesToVisit);

// Print array in reverse alphabetical order without modifying the original list
console.log("\nArray in reverse alphabetical order:");
console.log([...placesToVisit].sort().reverse());

// Print original array to show it's still unchanged
console.log("\nOriginal array (unchanged):");
console.log(placesToVisit);

// Reverse the order of the original list
placesToVisit.reverse();
console.log("\nReversed array:");
console.log(placesToVisit);

// Reverse the order of the list again to bring it back to its original order
placesToVisit.reverse();
console.log("\nArray reversed again (back to original order):");
console.log(placesToVisit);

// Sort the array in alphabetical order
placesToVisit.sort();
console.log("\nArray sorted in alphabetical order:");
console.log(placesToVisit);

// Sort the array in reverse alphabetical order
placesToVisit.sort((a, b) => b.localeCompare(a));
console.log("\nArray sorted in reverse alphabetical order:");
console.log(placesToVisit);
