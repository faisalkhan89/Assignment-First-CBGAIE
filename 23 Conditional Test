let age = 25;
console.log("Is age greater than 18? I predict True.");
console.log(age > 18); // True

let temperature = 20;
console.log("Is temperature less than or equal to 25? I predict True.");
console.log(temperature <= 25); // True

let city = "New York";
console.log("Is city equal to 'New York'? I predict True.");
console.log(city == "New York"); // True

let isSunny = false;
console.log("Is it sunny outside? I predict False.");
console.log(isSunny); // False

let username = "admin";
console.log("Is username not equal to 'guest'? I predict True.");
console.log(username != "guest"); // True

let isEven = 10 % 2 == 0;
console.log("Is 10 even? I predict True.");
console.log(isEven); // True

let isMorning = "morning" === "Morning";
console.log("Is 'morning' equal to 'Morning'? I predict False.");
console.log(isMorning); // False

let isWeekend = "Saturday" || "Sunday";
console.log("Is it the weekend? I predict True.");
console.log(isWeekend); // True

let isCloudy = true && false;
console.log("Is it cloudy and sunny? I predict False.");
console.log(isCloudy); // False

let isPositive = -5 > 0;
console.log("Is -5 greater than 0? I predict False.");
console.log(isPositive); // False
