let age = 25;
console.log("Is age greater than 18? I predict True.");
console.log(age > 18);

let temperature = 20;
console.log("Is temperature less than or equal to 25? I predict True.");
console.log(temperature <= 25);

let city = "New York";
console.log("Is city equal to 'New York'? I predict True.");
console.log(city == "New York");

let isSunny = false;
console.log("Is it sunny outside? I predict False.");
console.log(isSunny);

let username = "admin";
console.log("Is username not equal to 'guest'? I predict True.");
console.log(username != "guest");
